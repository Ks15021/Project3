{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-project3"
		},
		"LS_adls_csv_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adls_csv_sink'"
		},
		"LS_Git_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"LS_adls_csv_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdevproject3.dfs.core.windows.net/"
		},
		"Man_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/80bf3767-19f0-4694-9fca-d6ab03a4c69c/resourceGroups/RG_project_3/providers/Microsoft.Storage/storageAccounts/adlsdevproject3"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-project3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_adls_csv_sink_accountKey": {
						"value": "[parameters('LS_adls_csv_sink_accountKey')]"
					},
					"LS_Git_csv_properties_typeProperties_url": {
						"value": "[parameters('LS_Git_csv_properties_typeProperties_url')]"
					},
					"LS_adls_csv_sink_properties_typeProperties_url": {
						"value": "[parameters('LS_adls_csv_sink_properties_typeProperties_url')]"
					},
					"Man_trigger_properties_typeProperties_scope": {
						"value": "[parameters('Man_trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}